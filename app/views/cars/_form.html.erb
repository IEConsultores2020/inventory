<%= form_with model: [ @dealer, @dealer.cars.build ] do |form| %>


  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :yearmodel %>
    <%= form.number_field :yearmodel, in: 1885..2021 %>
  </div>

  <div class="field">
    <%= form.label :prices %>
    <%= form.number_field :price %>
  </div>



  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
